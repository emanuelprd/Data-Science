       "17  0.008376 -0.002230  0.001595 -0.000894  0.000382 -0.002815  \n",
       "18 -0.036643 -0.001242  0.014081 -0.019723 -0.000038  0.000281  \n",
       "19 -0.067583  0.001845 -0.001148  0.000490 -0.000203  0.001498  \n",
       "20  0.115493 -0.001547 -0.008567  0.013476  0.000296 -0.002179  \n",
       "21 -0.001547  0.147265 -0.044073 -0.030245 -0.000365  0.002687  \n",
       "22 -0.008567 -0.044073  0.066550 -0.068705  0.000200 -0.001471  \n",
       "23  0.013476 -0.030245 -0.068705  0.119518 -0.000057  0.000419  \n",
       "24  0.000296 -0.000365  0.000200 -0.000057  0.003017 -0.022222  \n",
       "25 -0.002179  0.002687 -0.001471  0.000419 -0.022222  0.163649  \n",
       "\n",
       "[26 rows x 26 columns]"
      ]
     },
     "execution_count": 11,
     "metadata": {},
     "output_type": "execute_result"
    }
   ],
   "source": [
    "# Matriz de Correspondência\n",
    "n = np.sum(burt.values)\n",
    "P = (burt/n).values\n",
    "\n",
    "# Massas\n",
    "r = np.sum(P, axis=1) # row\n",
    "c = np.sum(P, axis=0) # column\n",
    "\n",
    "# Matriz Diagonal\n",
    "D_r = np.diag(1 / np.sqrt(r)) # row\n",
    "D_c = np.diag(1 / np.sqrt(c)) # column\n",
    "\n",
    "# Muda o formato das matrizes de linha para coluna\n",
    "r = r.reshape(-1, 1)  \n",
    "c = c.reshape(1, -1) \n",
    "P_minus_rcT = P - r @ c # O operador @ realiza a mesma operação que np.dot()\n",
    "\n",
    "# Matriz de resíduos padronizados (S)\n",
    "S = D_r @ P_minus_rcT @ D_c\n",
    "\n",
    "print(\"Matrix S (Standardized Residuals)\")\n",
    "pd.DataFrame(S)"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 12,
   "id": "fec61dc3",
   "metadata": {},
   "outputs": [
    {
     "data": {
      "text/plain": [
       "array([0.31293442, 0.27829508, 0.23996773, 0.2208708 , 0.20101071])"
      ]
     },
     "execution_count": 12,
     "metadata": {},
     "output_type": "execute_result"
    }
   ],
   "source": [
    "# Singular Value Decomposition - SVD\n",
    "U, D_a, Vt = np.linalg.svd(S, full_matrices=False)\n",
    "D_a[:5]"
   ]
  },
